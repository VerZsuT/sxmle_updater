{
  "name": "snowrunner-xml-editor-desktop",
  "productName": "SnowRunner XML Editor",
  "version": "0.6.8b",
  "description": "Allows you to visually edit the game's XML files.",
  "repository": {
    "type": "git",
    "url": "https://github.com/VerZsuT/SnowRunner-XML-Editor-Desktop.git"
  },
  "main": ".webpack/main",
  "scripts": {
    "build": "node ./build_scripts/preBuild.js && npm run package-prod && node ./build_scripts/postBuild.js x32",
    "build-x64": "node ./build_scripts/preBuild.js && npm run package-prod-x64 && node ./build_scripts/postBuild.js x64",
    "package": "electron-forge package --platform=win32 --arch=ia32",
    "package-prod": "cross-env NODE_ENV=production electron-forge package --platform=win32 --arch=ia32",
    "package-prod-x64": "cross-env NODE_ENV=production electron-forge package --platform=win32 --arch=x64",
    "package-x64": "electron-forge package --platform=win32 --arch=x64",
    "start": "cross-env NODE_ENV=development electron-forge start",
    "texts": "node build_scripts/texts"
  },
  "keywords": [],
  "author": {
    "name": "VerZsuT",
    "email": "verzsut@gmail.com"
  },
  "license": "MIT",
  "config": {},
  "devDependencies": {},
  "dependencies": {},
  "optionalDependencies": {},
  "peerDependencies": {}
}
